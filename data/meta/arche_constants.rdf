    </acdh:RepoObject>
    
    <!-- Resource specific mandatory or very nice to have constant properties -->
    <acdh:SpecResource collection="editions">
        <acdh:hasCategory rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/text/tei"/> <!-- TEI encoded texts -->
    </acdh:SpecResource>
    <acdh:SpecResource collection="indices">
        <acdh:hasCategory rdf:resource="https://vocabs.acdh.oeaw.ac.at/archecategory/text/tei"/> <!-- TEI encoded texts -->
    </acdh:SpecResource>
    
    <!-- maps arche props to their values via xpath expressions; be aware the current TEI document needs to be encoded as '$item' -->
    <acdh:TeiLookUps collection="editions"> 
        <acdh:hasTitle type="literal">$item//tei:title[@type='label']/text()</acdh:hasTitle>
        <acdh:hasCoverageStartDate type="date">data($item//tei:titleStmt//tei:date/@when-iso)</acdh:hasCoverageStartDate>
        <acdh:continues type="resource">$item/@pref</acdh:continues>
        <acdh:hasPid type="literal_no_lang">$item//tei:idno[@type="handle"]/text()</acdh:hasPid>
        <acdh:isPartOf type="resource">$item/@xml:base</acdh:isPartOf>
        <acdh:hasDescription type="literal">normalize-space(string-join($item//tei:msContents//text()))</acdh:hasDescription>
        <acdh:hasCustomXsl type="no_eval">https://tei4arche.acdh-dev.oeaw.ac.at/xsl/thun2arche.xsl</acdh:hasCustomXsl>
    </acdh:TeiLookUps>
    <acdh:TeiLookUps collection="indices"> 
        <acdh:hasTitle type="literal">$item//tei:titleStmt/tei:title[1]/text()</acdh:hasTitle>
        <acdh:hasPid type="literal_no_lang">$item//tei:idno[@type="handle"]/text()</acdh:hasPid>
        <acdh:isPartOf type="resource">$item/@xml:base</acdh:isPartOf>
        <acdh:hasCustomXsl type="literal_no_lang">concat('https://tei4arche.acdh-dev.oeaw.ac.at/xsl/', substring-before(data($item/@xml:id), '.'), '.xsl')</acdh:hasCustomXsl>
    </acdh:TeiLookUps>
    
    <!-- mapping  tei:person to arche:Person -->
    <acdh:PersonLookUps source="indices/listperson.xml">
        <acdh:hasIdentifier type="resource_many">archeutils:get_entity_id($item)</acdh:hasIdentifier>
        <acdh:hasTitle type="literal" lang="und">normalize-space($item/tei:persName[1]/tei:forename/text()||' '||$item/tei:persName[1]/tei:surname/text())</acdh:hasTitle>
    </acdh:PersonLookUps>
    
    <!-- mapping  tei:place to arche:Place -->
    <acdh:PlaceLookUps source="indices/listplace.xml">
        <acdh:hasIdentifier type="resource_many">archeutils:get_entity_id($item)</acdh:hasIdentifier>
        <acdh:hasTitle type="literal" lang="und">normalize-space($item/tei:placeName[1]/text())</acdh:hasTitle>
    </acdh:PlaceLookUps>
    
    <!-- mapping  tei:org to arche:Organisation -->
    <acdh:OrgLookUps source="indices/listorg.xml">
        <acdh:hasIdentifier type="resource_many">archeutils:get_entity_id($item)</acdh:hasIdentifier>
        <acdh:hasTitle type="literal" lang="und">normalize-space($item/tei:orgName[1]/text())</acdh:hasTitle>
    </acdh:OrgLookUps>
</acdh:ACDH>